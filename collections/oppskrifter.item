              </div>
            {.end}
            {.if customContent.engine}
              <div class="item">
                <div class="title">
                  Main engine
                </div>
                <div class="data">
                  {engine}
                </div>
              </div>
            {.end}
            {.if customContent.propulsion}
              <div class="item">
                <div class="title">
                  Propulsion
                </div>
                <div class="data">
                  {propulsion}
                </div>
              </div>
            {.end}
        </div>
      {.end}
    </div>
  </section>

  <section id="boat-content" class="Index-page">
    <div class="Index-page-content">  
        {body}
        <div class="BlogItem-share">{@|apply sqs-share-buttons}</div>
        {.if postItemInjectCode}{postItemInjectCode}{.end}   
    </div>
  </section>
{.end}
  
       


      

  



    

