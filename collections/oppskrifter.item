                  {number}
                </div>
              </div>
            {.end}
            {.if customContent.year}
              <div class="item">
                <div class="title">
                  Year
                </div>
                <div class="data">
                  {year}
                </div>
              </div>
            {.end}
            {.if customContent.pax}
              <div class="item">
                <div class="title">
                  Pax
                </div>
                <div class="data">
                  {pax}
                </div>
              </div>
            {.end}
            {.if customContent.name}
              <div class="item">
                <div class="title">
                  Name
                </div>
                <div class="data">
                  {name}
                </div>
              </div>
            {.end}
            {.if customContent.operator}
              <div class="item">
                <div class="title">
                  Operator 
                </div>
                <div class="data">
